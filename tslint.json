{
    "rulesDirectory": [
        "src"
    ],
    "rules": {
        "max-line-length": {
            "options": [
                90
            ]
        },
        "quotemark": [
            "single",
            "avoid-template"
        ],
        "no-console": [
            true,
            "log",
            "warning"
        ],
        "no-var-keyword": true,
        "switch-default": true,
        "indent": [
            true,
            "spaces"
        ],
        "ban": [
            true,
            "google"
        ]
    },
    "jsRules": {
        "max-line-length": {
            "options": [
                90
            ]
        }
    }
}